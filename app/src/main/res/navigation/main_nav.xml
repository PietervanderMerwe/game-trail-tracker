<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/my_nav"
    app:startDestination="@id/dashboardFragment">

    <fragment
        android:id="@+id/addFragment"
        android:name="com.epilogs.game_trail_tracker.fragments.add.AddFragment"
        android:label="fragment_add"
        tools:layout="@layout/fragment_add" >
        <action
            android:id="@+id/action_addFragment_to_viewFragment"
            app:destination="@id/viewFragment" />
        <action
            android:id="@+id/action_addFragment_to_dashboardFragment"
            app:destination="@id/dashboardFragment" />
    </fragment>
    <fragment
        android:id="@+id/viewFragment"
        android:name="com.epilogs.game_trail_tracker.fragments.view.ViewFragment"
        android:label="fragment_view"
        tools:layout="@layout/fragment_view" >
        <action
            android:id="@+id/action_viewFragment_to_addFragment"
            app:destination="@id/addFragment" />
        <action
            android:id="@+id/action_viewFragment_to_dashboardFragment"
            app:destination="@id/dashboardFragment" />
        <action
            android:id="@+id/action_viewFragment_to_locationViewDetailFragment"
            app:destination="@id/locationViewDetailFragment2" />
        <action
            android:id="@+id/action_viewFragment_to_weaponViewDetailFragment"
            app:destination="@id/weaponViewDetailFragment2" />
        <action
            android:id="@+id/action_viewFragment_to_animalViewDetailFragment"
            app:destination="@id/animalViewDetailFragment2" />
    </fragment>
    <fragment
        android:id="@+id/dashboardFragment"
        android:name="com.epilogs.game_trail_tracker.fragments.hunt.HuntFragment"
        android:label="fragment_dashboard"
        tools:layout="@layout/fragment_hunt" >
        <action
            android:id="@+id/action_dashboardFragment_to_viewFragment"
            app:destination="@id/viewFragment" />
        <action
            android:id="@+id/action_dashboardFragment_to_addFragment"
            app:destination="@id/addFragment" />
        <action
            android:id="@+id/action_dashboardFragment_to_animalViewDetailFragment2"
            app:destination="@id/animalViewDetailFragment2" />
        <action
            android:id="@+id/action_dashboardFragment_to_locationViewDetailFragment2"
            app:destination="@id/locationViewDetailFragment2" />
    </fragment>
    <fragment
        android:id="@+id/animalViewDetailFragment2"
        android:name="com.epilogs.game_trail_tracker.fragments.trophy.TrophyViewDetailFragment"
        android:label="fragment_animal_view_detail"
        tools:layout="@layout/fragment_trophy_view_detail" >
        <argument
            android:name="animalId"
            app:argType="integer" />
        <action
            android:id="@+id/action_animalViewDetailFragment_to_viewFragment"
            app:destination="@id/viewFragment" />
        <action
            android:id="@+id/action_animalViewDetailFragment2_to_locationViewDetailFragment2"
            app:destination="@id/locationViewDetailFragment2" />
        <action
            android:id="@+id/action_animalViewDetailFragment2_to_weaponViewDetailFragment2"
            app:destination="@id/weaponViewDetailFragment2" />
        <action
            android:id="@+id/action_animalViewDetailFragment2_to_dashboardFragment"
            app:destination="@id/dashboardFragment" />
    </fragment>
    <fragment
        android:id="@+id/locationViewDetailFragment2"
        android:name="com.epilogs.game_trail_tracker.fragments.hunt.HuntViewDetailFragment"
        android:label="fragment_location_view_detail"
        tools:layout="@layout/fragment_hunt_view_detail" >
        <argument
            android:name="locationId"
            app:argType="integer" />
        <action
            android:id="@+id/action_locationViewDetailFragment_to_viewFragment"
            app:destination="@id/viewFragment" />
        <action
            android:id="@+id/action_locationViewDetailFragment2_to_animalViewDetailFragment2"
            app:destination="@id/animalViewDetailFragment2" />
        <action
            android:id="@+id/action_locationViewDetailFragment2_to_dashboardFragment"
            app:destination="@id/dashboardFragment" />
    </fragment>
    <fragment
        android:id="@+id/weaponViewDetailFragment2"
        android:name="com.epilogs.game_trail_tracker.fragments.weapon.WeaponViewDetailFragment"
        android:label="fragment_weapon_view_detail"
        tools:layout="@layout/fragment_weapon_view_detail" >
        <argument
            android:name="weaponId"
            app:argType="integer" />
        <action
            android:id="@+id/action_weaponViewDetailFragment_to_viewFragment"
            app:destination="@id/viewFragment" />
        <action
            android:id="@+id/action_weaponViewDetailFragment2_to_animalViewDetailFragment2"
            app:destination="@id/animalViewDetailFragment2" />
    </fragment>
</navigation>